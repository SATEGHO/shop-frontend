{
  "$schema": "http://json.schemastore.org/prettierrc",
  "bracketSpacing": true,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "arrowParens": "always",
  "printWidth": 100,
  "quoteProps": "as-needed",
  "singleAttributePerLine": false,
  "bracketSameLine": true
}
